<!DOCTYPE html>
<html>
<head>
	<title>Odd Numbers Less than 100</title>
</head>
<body>
<button onclick="myFunction()">While</button>
<button onclick="myFunction1()">do...while</button>
<button onclick="myFunction2()">for loop</button>
<P id="demo"></P>
<script type="text/javascript">
	function myFunction() {
			var i=1;
			var txt="";
			while(i<100)
			{
				if (i%2!=0) {
				txt=txt+i+"<br>";	
				}
				i++;
			}
			document.getElementById('demo').innerHTML=txt;
			}
	function myFunction1() {
				var i=1;
				var txt="";
				do{
					if (i%2!=0) {
						txt=txt+i+"<br>";
					}
					i++
				}while(i<100);
				document.getElementById('demo').innerHTML=txt;
			}	
	function myFunction2() {
		for(var i=1,txt="";i<100;i++)
		{
			if (i%2!=0) {
				txt=txt+i+"<br>";
			}
		}
		document.getElementById('demo').innerHTML=txt;
	}
</script>
</body>
</html>
